@model IEnumerable<dev_backend_habitly_eixo2.Models.Etiqueta
>

@{
    ViewData["Title"] = "Index";
}

<h1>Etiquetas</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.First().Nome)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
    <tr>
        <td>@Html.DisplayFor(m => item.Nome)</td>
        <td>
            <a asp-action="Details" asp-route-id="@item.IdEtiqueta">Details</a> |
            <a asp-action="Edit" asp-route-id="@item.IdEtiqueta">Edit</a> |
            <a asp-action="Delete" asp-route-id="@item.IdEtiqueta">Delete</a>
        </td>
    </tr>
}
    </tbody>
</table>
